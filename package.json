    "jest": {
        "setupFiles": [
            "./src/__tests__/setupJest.js"
        ],
        "modulePaths": [
            "./src"
        ],
        "moduleNameMapper": {
            "^.*\\.scss$": "__tests__/SCSSStub.js"
        },
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        },
        "coverageReporters": [
            "lcov"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/",
            "!src/assets/**/*.{js,jsx}",
            "!src/config/**/*.{js,jsx}"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
        ],
        "testPathIgnorePatterns": [
            "./node_modules/",
            "./src/__tests__/setupJest.js",
            "./src/__tests__/SCSSStub.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    }
